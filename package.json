{
  "name": "@onpe/ui",
  "version": "1.0.26",
  "type": "module",
  "description": "Librería completa de UI para ONPE - Componentes, Hooks, Utils y Librerías",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "onpe-ui": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components.esm.js",
      "require": "./dist/components.js",
      "types": "./dist/components.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.esm.js",
      "require": "./dist/hooks.js",
      "types": "./dist/hooks.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.esm.js",
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.esm.js",
      "require": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    },
    "./styles": "./dist/index.css",
    "./tailwind-preset": "./tailwind-preset.js"
  },
  "files": [
    "dist",
    "!dist/**/*.stories.*",
    "!dist/**/IconsOverview.*",
    "!dist/**/*.map"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "components",
    "hooks",
    "utils",
    "lib",
    "tailwind",
    "ui",
    "typescript",
    "onpe",
    "peru",
    "elecciones"
  ],
  "author": "ONPE",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardosv46/onpe-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ricardosv46/onpe-ui/issues"
  },
  "homepage": "https://github.com/ricardosv46/onpe-ui#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.6.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.3.1"
  }
}
